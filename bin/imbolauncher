 */

$parent = __DIR__;

while (($parent = dirname($parent)) !== '/') {
    if (file_exists($parent . "/vendor/autoload.php")) {
        break;
    }
}

require $parent . '/vendor/autoload.php';

$app = new ImboLauncher\Application();
$app->run();
